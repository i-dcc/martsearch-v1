({
  url:                  '/biomart/biomart/martservice',
  name:                 'Ensembl (Mus Musculus)',
  description:          '',
  datasetConfigVersion: 0.6,
  dataset_name:         'mmusculus_gene_ensembl',
  importables:
    [
      { name: 'ensembl_gene_id' }
    ],
  exportables:
    [
      { name: 'ensembl_gene_id' }
    ],
  filters:
    [
      { name: 'ensembl_gene_id',  display: 'Ensembl Gene ID' },
      { name: 'mgi_symbol',       display: 'Gene Symbol', enabled: true }
    ],
  attributes:
    [
      { name: 'mgi_symbol',       display: 'Gene Symbol',       enabled: true },
      { name: 'ensembl_gene_id',  display: 'Ensembl Gene ID',   enabled: true },
      { name: 'description',      display: 'Description',       enabled: true },
    ],
  template: new String(
    '<strong>{ $T.source.name }</strong>'+
    '<table>'+
      '<thead>'+
        '<tr>'+
        '{#foreach $T.source.attributes as attribute}'+
          '{#if $T.attribute.enabled}'+
          '<th>{$T.attribute.display}</th>'+
          '{#/if}'+
        '{#/for}'+
        '</tr>'+
      '</thead>'+
      '<tbody>'+
        '{#foreach $T.results as result_array}'+
          '<tr>'+
          '{#foreach $T.result_array as result}'+
              '<td>{$T.result}</td>'+
          '{#/for}'+
          '</tr>'+
        '{#/for}'+
      '</tbody>'+
    '</table>'
  )
})
