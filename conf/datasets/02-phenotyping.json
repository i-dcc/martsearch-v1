  post_display_hook: function ( content_id ) {
    jQuery('#'+content_id).find("td[rel^='qtip']").each( function() {
      jQuery(this).qtip({
         content: jQuery(this).attr('tooltip'),
         show: 'mouseover',
         hide: 'mouseout',
         style: {
           name: 'dark'
         },
         position: {
           corner: {
             target: 'topLeft',
             tooltip: 'bottomRight'
           }
         }
      });
    });
  }
